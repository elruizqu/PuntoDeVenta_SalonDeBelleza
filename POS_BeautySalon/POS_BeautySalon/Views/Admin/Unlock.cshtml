
@model DAL.ApplicationUser



<h2>Desbloquear Usuario</h2>

<div>
    <h3>¿Estás seguro que quieres desbloquear a @Model.Email?</h3>
    <form asp-action="UnlockConfirmed" method="post">

        <input type="hidden" asp-for="Id" />
        <button type="submit" class="btn btn-dark">Desbloquear</button>
        <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
    </form>
</div>
<br>
<br>
<div>
    <a asp-action="Index">Volver</a>
</div>